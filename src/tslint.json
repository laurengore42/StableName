{
    "extends": "../tslint.json",
    "rules": {
        "max-line-length": false,
        "variable-name": false,
        "prefer-const": false,
        "directive-selector": [
            true,
            "attribute",
            "sn",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "sn",
            "kebab-case"
        ]
    }
}
